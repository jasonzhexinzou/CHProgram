<script type="text/javascript">
    var startdate = '@ViewBag.StartDate';
    var enddate = '@ViewBag.EndDate';
    //列表面板 按数量金额
    var allshowlist = [];
    var susshowlist = [];
    var refshowlist = [];
    var penshowlist = [];
    var canshowlist = [];
    //总金额总数量 按数量金额
    var allLab = [];
    var LabSus = [];
    var LabRef = [];
    var LabPen = [];
    var LabCan = [];
    //饼图 按数量金额
    var Chartall = [];
    var Chartsus = [];
    var Chartref = [];
    var Chartpen = [];
    var Chartcan = [];
    //列表面板 按审批状态
    var allshow = [];
    var susshow = [];
    var refshow = [];
    var penshow = [];
    var canshow = [];
    //总数量 按审批状态
    var allLablist = [];
    var LabSuslist = [];
    var LabReflist = [];
    var LabPenlist = [];
    var LabCanlist = [];
    //饼图 按审批状态
    var Chartalllist = [];
    var Chartsuslist = [];
    var Chartreflist = [];
    var Chartpenlist = [];
    var Chartcanlist = [];

    $(document).ready(function () {
        var calendar = new LCalendar();
        calendar.init({
            'trigger': '#startDate', //标签id
            'type': 'date', //date 调出日期选择 datetime 调出日期时间选择 time 调出时间选择 ym 调出年月选择,
            'minDate': (new Date().getFullYear() - 3) + '-' + 1 + '-' + 1, //最小日期
            'maxDate': (new Date().getFullYear() + 3) + '-' + 12 + '-' + 31 //最大日期
        });

        var calendar = new LCalendar();
        calendar.init({
            'trigger': '#endDate', //标签id
            'type': 'date', //date 调出日期选择 datetime 调出日期时间选择 time 调出时间选择 ym 调出年月选择,
            'minDate': (new Date().getFullYear() - 3) + '-' + 1 + '-' + 1, //最小日期
            'maxDate': (new Date().getFullYear() + 3) + '-' + 12 + '-' + 31 //最大日期
        });

