        var countData = [];
        getJson();

        function getJson() {
            var _StartDate = $('#startDate').val();
            var _EndDate = $('#endDate').val();
            showLoadingToast();
            post('/P/CostAnalysis/LoadPreApprovalData',
                {
                    StartDate: _StartDate,
                    EndDate: _EndDate
