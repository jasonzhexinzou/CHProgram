                tooltip: {
                    trigger: 'item',
                    formatter: '{a} <br/>{b} '
                },

                series: [
                    {
                        name: 'HT订单金额占比',
                        type: 'pie',
                        radius: '45%',
                        center: ['50%', '40%'],
