        function DrawBarAmount(CountX_1, CountX_2, CountY) {
            option = {
                backgroundColor: 'rgb(255, 255, 255)',
                color: ['#F66800', '#006774'],
                height: 220,
                tooltip: {
                    trigger: 'axis',
                    axisPointer: {
                        type: 'shadow'
                    },
                    trigger: 'item',
                    formatter: function (data) {
                        var result = [],
                            counter = 0,
                            num = data.value;
                        num = (num || 0).toString().split('');
                        for (var i = num.length - 1; i >= 0; i--) {
                            counter++;
                            result.unshift(num[i]);
                            //无小数部分
                            if (!(counter % 3) && i != 0) {
                                result.unshift(',');
                            }
                        }
                        result = result.join('');
                        return data.name + " : " + result;
                    }
                },
                legend: {
                    data: ['HT预算金额', 'HT订单金额']
                },
                grid: {
                    left: '3%',
                    right: '4%',
                    bottom: '3%',
                    containLabel: true
                },
                xAxis: {
                    type: 'value',
                    boundaryGap: [0, 0.01]
                },
                yAxis: {
                    type: 'category',
                    inverse: true,//倒叙
                    data: CountY,
                    axisLabel: {
                        interval: 0,
                        fontSize: 10,
                        formatter: function (value) {
                            return value.split("-").join("\n\n");
                        }
                    }
